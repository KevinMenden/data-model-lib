{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://qbic.life/v1/ont-metadata.schema.json",
  "title": "Nanopore Instrument Metadata Output",
  "description": "Describes in which form nanopore metadata is received from the lab.",
  "type": "object",
  "properties": {
    "asic_temp": {
      "type": "string",
      "minLength": 1
    },
    "instrument": {
      "type": "string",
      "minLength": 1
    },
    "flowcell_id": {
      "type": "string",
      "minLength": 1
    },
    "flowcell_position": {
      "type": "string",
      "minLength": 1
    },
    "flowcell_type": {
      "type": "string",
      "minLength": 1
    },
    "protocol": {
      "type": "string",
      "pattern": "SQK-.*(?=:)"
    },
    "hostname": {
      "type": "string",
      "minLength": 1
    },
    "start_date": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": [
    "asic_temp",
    "instrument",
    "flowcell_id",
    "flowcell_position",
    "flowcell_type",
    "protocol",
    "hostname",
    "start_date"
  ]
}